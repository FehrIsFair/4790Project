datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Anime {
  id Int @id @default(autoincrement())
  titleId Int
  title Title @relation(fields: [titleId], references: [id])
  
}

model Title {
  id Int @id @default(autoincrement())
  romanji String
  english String
  native String
  userPreferred String
}